@{
    ViewData["Title"] = "Home Page";
}
<h1>Lost in the woods</h1>
<h2>List of local hiking trails</h2>
<table>
    <tr>
        <th>Trail Name</th>
        <th>Trail Length</th>
        <th>Elevation Change</th>
    </tr>
</table>
@{
    foreach (Trail trail in ViewBag.Trails)
    {
        <tr>
            <td>@trail.Name</td>
            <td>@trail.Length/td>
            <td>@trail.Elevation</td>
        </tr>
    }
}
<button id="new_trail_button">Add a new trail</button>